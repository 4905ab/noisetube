  <body onload="initialize()" onunload="GUnload()">
    <div id="container">
    <div class="row">
      <table><tr><td><a href="http://www.noisetube.net"><img alt="NoiseTube" src="/images/noisetubelogo.png" /></a></td>
      <td align="right" style="width:500px"></td></tr></table>

       <div id="navs">
        <div class="clearfix" style="float:left;"><ul id="nav" >      
          <% if logged_in? %>
            <li>Welcome <%= current_user.login %></li>
            <li><a href="/users/">community</a></li>
          <% else %>
            
          <% end %>
          
        </ul>
        </div>
        <div class="clearfix" style="float:right;">
          <ul id="nav" >
            <% if logged_in? %>
              <li><%= link_to 'your Elog', :controller => 'users', :action=>"edit", :id => current_user  %></li>
              <li><%= link_to 'logout', logout_path %></li>
             <% else
              if RAILS_ENV=='development' %>
              <li><%= link_to 'login', login_path %></li>
              <li><%= link_to 'sign up', signup_path %></li>
           <% end
           end %>
          </ul>
        </div>
      </div>
