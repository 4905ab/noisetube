<%
xml = Builder::XmlMarkup.new(:indent=>2)
xml.instruct!
xml.rss(:version => "2.0", "xmlns:georss" => GEORSS_NS) do
  xml.channel do
    xml.title "Digital traces of #{@user.login}'s exposure to noise pollution"
    xml.link(user_url(@user))
    
    xml.description "#{@user.login}'s exposure stream"
    to_georss(xml,@tracks)
    
    
  end
end
%><%= xml.target! %>